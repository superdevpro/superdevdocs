{
	"plugins": ["prettier-plugin-tailwindcss"],
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": false,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": true,
	"printWidth": 120,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"singleAttributePerLine": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useEditorConfig": false,
	"useTabs": true
}
